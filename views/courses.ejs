<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Courses</title>
  </head>
  <body>
    <h1>Courses offered by <%= university.name %>:</h1>

    <ul>
      <% courses.forEach(course => { %>
      <li><%= course.name %> - <%= course.type %></li>
      <% }); %>
    </ul>
  </body>
</html>
